    "from passlib.context import CryptContext\n",
    "\n",
    "pwd_context = CryptContext(schemes=[\"bcrypt\"], deprecated=\"auto\")\n",
    "\n",
    "def get_password_hash(password: str) -> str:\n",
    "     return pwd_context.hash(password)"
   ]
